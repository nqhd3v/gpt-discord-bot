{
  "cSpell.words": [
    "davinci",
    "openai"
  ],
  "files.associations": {
    "*.js": "javascript",
  },
  "editor.formatOnSave": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "public/fontawesome": true
  },
  "workbench.colorCustomizations": {
    "activityBar.background": "#006a57",
    "activityBar.activeBackground": "#006a57",
    "activityBar.foreground": "#ffffff",
    "activityBar.inactiveForeground": "#fae4f6",
    "activityBar.activeBorder": "#ffffff",
    "activityBarBadge.background": "#fae4f6",
    "activityBarBadge.foreground": "#006a57",
    "titleBar.activeBackground": "#006a57",
    "titleBar.inactiveBackground": "#006a57",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveForeground": "#e7e7e799",
    "statusBar.background": "#006a57",
    "statusBarItem.hoverBackground": "#626262",
    "statusBar.foreground": "#ffffff",
  },
  "workbench.preferredLightColorTheme": "Default Dark+"
}
